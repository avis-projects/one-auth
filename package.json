  },
  "devDependencies": {
    "@types/express": "^4.17.18",
    "nodemon": "^3.0.1",
    "serverless-offline": "^13.3.3",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^5.2.2"
  }
